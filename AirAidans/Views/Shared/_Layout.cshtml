                        </li>
                        <li class="nav-item custom-li">
                            <a href="@Url.Action("Account","Home")" class="nav-link">Account</a>
                        </li>
                        <li class="nav-item custom-li">
                            <a href="@Url.Action("Index","Contact")" class="nav-link">Contact</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
